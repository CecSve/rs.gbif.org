<?xml version="1.0" encoding="UTF-8"?>
<searchTemplate xmlns="http://rs.tdwg.org/tapir/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://rs.tdwg.org/tapir/1.0 http://rs.tdwg.org/tapir/1.0/schema/tapir.xsd">
	<label>Full Darwin 1.4 record based on scientific name</label>
	<documentation>Return a full Darwin Core 1.4 record given the scientific name parameter.</documentation>
	<externalOutputModel location="http://rs.tdwg.org/tapir/cs/dwc/1.4/model/dw_core_geo_cur.xml"/>
	<filter>
		<equals>
			<concept id="http://rs.tdwg.org/dwc/dwcore/ScientificName"/>
			<parameter name="sname"/>
		</equals>
	</filter>
</searchTemplate>
