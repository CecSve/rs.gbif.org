<concept dc:identifier="originalDesignation" dc:URI="http://rs.gbif.org/vocabulary/gbif/type_designation_type/originalDesignation" 
 dc:relation="http://rs.gbif.org/vocabulary/gbif/type_designation_type/originalDesignation"
 dc:description="If one nominal species is explicitly designated as the type species when a nominal genus-group taxon is established, that nominal species is the type species (type by original designation).">
 <preferred>
  <term dc:title="Original Designation" xml:lang="en"/>
 </preferred>
 <alternative>
 </alternative>
</concept>

