<concept dc:identifier="tautonymy" dc:URI="http://rs.gbif.org/vocabulary/gbif/type_designation_type/tautonymy" 
 dc:relation="http://rs.gbif.org/vocabulary/gbif/type_designation_type/tautonymy"
 dc:description="If a valid species-group name, or its cited synonym, originally included in a nominal genus-group taxon is identical with the name of that taxon, the nominal species denoted by that specific name (if available) is the type species.">
 <preferred>
  <term dc:title="Tautonymy" xml:lang="en"/>
 </preferred>
 <alternative>
 </alternative>
</concept>

<concept dc:identifier="absoluteTautonymy" dc:URI="http://rs.gbif.org/vocabulary/gbif/type_designation_type/absoluteTautonymy" 
 dc:relation="http://rs.gbif.org/vocabulary/gbif/type_designation_type/absoluteTautonymy"
 dc:description="The identical spelling of a generic or subgeneric name and the specific or subspecific name of one of its originally included nominal species or subspecies.">
 <preferred>
  <term dc:title="Absolute Tautonymy" xml:lang="en"/>
 </preferred>
 <alternative>
 </alternative>
</concept>

<concept dc:identifier="linnaeanTautonymy" dc:URI="http://rs.gbif.org/vocabulary/gbif/type_designation_type/linnaeanTautonymy" 
 dc:relation="http://rs.gbif.org/vocabulary/gbif/type_designation_type/linnaeanTautonymy"
 dc:description="The identical spelling of a new generic or subgeneric name established before 1931 and a pre-1758 name cited as a synonym of only one of the species or subspecies originally included in that genus.">
 <preferred>
  <term dc:title="Linnaean Tautonymy" xml:lang="en"/>
 </preferred>
 <alternative>
 </alternative>
</concept>

