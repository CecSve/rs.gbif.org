<concept dc:identifier="monotypy" dc:URI="http://rs.gbif.org/vocabulary/gbif/type_designation_type/monotypy" 
 dc:relation="http://rs.gbif.org/vocabulary/gbif/type_designation_type/monotypy"
 dc:description="Type species by monotypy. When an author establishes a new nominal genus-group taxon for a single taxonomic species and denotes that species by an available name, the nominal species so named is the type species.">
 <preferred>
  <term dc:title="Monotypy" xml:lang="en"/>
 </preferred>
 <alternative>
 </alternative>
</concept>

