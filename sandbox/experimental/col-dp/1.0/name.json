  "name": "name",
  "title": "Name",
  "fields": [
    {
      "name": "ID",
      "type": "string",
      "format": "default",
      "constraints": {
        "required": true
      }
    },
    {
      "name": "sourceID",
      "type": "string",
      "format": "default"
    },
    {
      "name": "basionymID",
      "type": "string",
      "format": "default"
    },
    {
      "name": "scientificName",
      "type": "string",
      "format": "default",
      "constraints": {
        "required": true
      }
    },
    {
      "name": "authorship",
      "type": "string",
      "format": "default"
    },
    {
