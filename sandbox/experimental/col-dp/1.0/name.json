    {
      "name": "uninomial",
      "type": "string",
      "format": "default"
    },
    {
      "name": "genus",
      "type": "string",
      "format": "default",
      "constraints": {
        "pattern": "^[A-Z\\p{Lu}]\\p{L}+$"
      }
    },
    {
      "name": "infragenericEpithet",
      "type": "string",
      "format": "default"
    },
    {
      "name": "specificEpithet",
      "type": "string",
      "format": "default"
