    {
      "name": "infraspecificEpithet",
      "type": "string",
      "format": "default"
    },
    {
      "name": "cultivarEpithet",
      "type": "string",
      "format": "default"
    },
    {
      "name": "code",
      "type": "string",
      "format": "default",
      "constraints": {
        "enum": [
          "bacterial",
          "botanical",
          "cultivars",
          "phytosociological",
          "virus",
          "zoological"
