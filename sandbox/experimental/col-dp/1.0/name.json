          "variety",
          "subvariety",
          "form",
          "subform",
          "pathovar",
          "biovar",
          "chemovar",
          "morphovar",
          "phagovar",
          "serovar",
          "chemoform",
          "forma specialis",
          "cultivar",
          "strain",
          "other",
          "unranked"
