  <!ENTITY tdwgVoc            "http://rs.tdwg.org/ontology/voc/">
  <!ENTITY ncdConcept         "http://rs.gbif.org/terms/ncd/ncdConcept#">
  <!ENTITY ncdType            "http://rs.gbif.org/terms/ncd/ncdType#">
  <!ENTITY history            "http://rs.gbif.org/terms/ncd/archive/">
  <!ENTITY contact            "http://rs.gbif.org/terms/ncd/ncdContact#">
]>

<rdf:RDF
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
  xmlns:owl="http://www.w3.org/2002/07/owl#"
  xmlns:skos="http://www.w3.org/2004/02/skos/core#"
  xmlns:foaf="http://xmlns.com/foaf/0.1/"
  xmlns:vs="http://www.w3.org/2003/06/sw-vocab-status/ns#"
  xmlns:vann="http://purl.org/vocab/vann/"
  xmlns:dc="http://purl.org/dc/elements/1.1/"
  xmlns:dcterms="http://purl.org/dc/terms/"
  xmlns:dcmitype="http://purl.org/dc/dcmitype/"
  xmlns:geo="http://www.w3.org/2003/01/geo/wgs84_pos#"
  xmlns:dwc="http://rs.tdwg.org/dwc/terms/"
  xmlns:ncd="&tdwgCollection;"
  xmlns:ncdConcept="&ncdConcept;"
  xmlns:ncdType="&ncdType;"
  xml:base="&ncdType;"
>


<!-- ### Vocabulary of type concepts (skos:ConceptScheme) ### -->

  <skos:ConceptScheme rdf:about="&ncdType;">
    <skos:prefLabel xml:lang="en">List of controlled values for the Natural Collections Description (NCD)</skos:prefLabel>
    <skos:definition xml:lang="en">Natural Collections Description (NCD) is a data standard for describing collections of natural history materials at the collection level; one NCD record describes one entire collection.</skos:definition>
    <dcterms:created>2007-03-05</dcterms:created>
    <dcterms:modified>2012-08-14</dcterms:modified>
    <dc:creator rdf:resource="&contact;GBIF"/>
    <dcterms:contributor rdf:resource="&contact;TDWG_NCD_IG"/>
    <dcterms:contributor rdf:resource="&contact;MarkusDoring"/>
    <dcterms:contributor rdf:resource="&contact;ConstanceRinaldo"/>
    <dcterms:contributor rdf:resource="&contact;DagEndresen"/>
    <dcterms:publisher rdf:resource="&contact;GBIF"/> <!-- who is the publisher ?? -->
    <dcterms:license rdf:resource="http://creativecommons.org/publicdomain/zero/1.0/"/> <!-- NB! Licence rights...? -->
    <dcterms:rights xml:lang="en">Creative Commons Public Domain license; see http://creativecommons.org/publicdomain/zero/1.0/. You are free to copy, distribute and transmit the work; to remix/adapt the work (e.g. to import the ontology and create specializations of its elements), all without asking permissions. If you wish to provide attribution you may include the statement: "This work is based on the Darwin Core extension for genebanks".</dcterms:rights>
    <dcterms:accessRights>public</dcterms:accessRights>
    <vann:preferredNamespaceUri>&tdwgCollectionType;</vann:preferredNamespaceUri>
    <vann:preferredNamespacePrefix>ncdType</vann:preferredNamespacePrefix>
    <foaf:homepage rdf:resource="http://www.tdwg.org/activities/ncd/"/>
    <dcterms:bibliographicCitation>TDWG NCD Interest Group (2009). Natural Collections Descriptions (NCD): A data standard for exchanging data describing natural history collections (Cover Page). Biodiversity Information Standards (TDWG). Available at http://www.tdwg.org/standards/312/, verified 20 June 2012.</dcterms:bibliographicCitation>
    <dcterms:bibliographicCitation>Thomson N, Hyam R, Rinaldo C, Butler C, Holland D, Mathe B, Waibel B, Addink W, Altenburg R, and DÃ¶ring M (2009). Natural Collections Description (NCD): A data standard for exchanging data describing natural history collections. Biodiversity Information Standards (TDWG). Available at http://www.tdwg.org/standards/312/, verified 20 June 2012.</dcterms:bibliographicCitation>
    <skos:note xml:lang="en">Notice that the base URI for the NCD vocabulary is "http://rs.tdwg.org/ontology/voc/Collection". Please make sure that you are only using element identifiers in this namespace, e.g. "http://rs.tdwg.org/ontology/voc/CollectionTERM". There may be copies of the vocabulary file on the Web which can be retrieved from other locations, but those locations should not be used as the basis of identifiers.</skos:note>
    <skos:editorialNote>Terms and definitions for this vocabulary of terms was extracted from the documentation NCD version 0.9 and from the TDWG Ontology.</skos:editorialNote>
    <rdfs:seeAlso rdf:resource="http://kos.gbif.org/wiki/NCD"/>
    <rdfs:seeAlso rdf:resource="http://www.tdwg.org/standards/312/"/>
    <rdfs:seeAlso rdf:resource="http://www.tdwg.org/activities/ncd/"/>
    <owl:versionInfo xml:lang="en">Draft version, not yet published</owl:versionInfo>
    <dcterms:hasVersion rdf:resource="&history;20120814/ncdType.rdf"/> <!-- SKOS version of the NCD here -->
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
  </skos:ConceptScheme>


  <!-- NCD controlled values (type concepts) are described by	
   skos:prefLabel	
   dc:description	
   vann:termGroup	
   rdf:type	
   rdfs:isDefinedBy	
   skos:inScheme	
   dcterms:modified	
   skos:example	
   skos:note	
   skos:editorialNote	
  -->





  <!-- Collection type -->

  <skos:Concept rdf:about="http://rs.tdwg.org/ontology/voc/CollectionType#CollectionTypeTerm">
    <skos:prefLabel xml:lang="en">Collection type</skos:prefLabel>
    <dc:description xml:lang="en">The class for controlled vocabularies of collection types. Picklist keyword to indicate typical contents or unifying characteristics of the collection.</dc:description>
    <vann:termGroup rdf:resource="http://rs.tdwg.org/ontology/voc/CollectionType#CollectionTypeTerm"/>
    <rdf:type rdf:resource="owl:Class"/>
    <rdfs:isDefinedBy rdf:resource="http://rs.tdwg.org/ontology/voc/CollectionType"/>
    <skos:inScheme rdf:resource="http://rs.gbif.org/terms/ncd/ncdType.rdf"/>
    <skos:example></skos:example>
    <skos:note></skos:note>
    <skos:editorialNote></skos:editorialNote>	
  </skos:Concept>

  <!--
  WARNING: Some of the NCD concepts for CollectionType in the NCD 0.9 are missing in the TDWG ontology (Collection.rdf): 
    geological, herbarium, manuscripts, mineralogical, specimens
  These concepts are included here in this SKOS vocabulary.
  -->

  <skos:Concept rdf:about="http://rs.tdwg.org/ontology/voc/CollectionType#archival">
    <skos:prefLabel xml:lang="en">Archival </skos:prefLabel>
    <dc:description xml:lang="en">Public and/or private records and historical documents  in various media which are preserved and accessible.</dc:description>
    <vann:termGroup rdf:resource="http://rs.tdwg.org/ontology/voc/CollectionType#CollectionTypeTerm"/>
    <rdf:type rdf:resource="rdfs:Datatype"/>
    <rdfs:isDefinedBy rdf:resource="http://rs.tdwg.org/ontology/voc/CollectionType"/>
    <skos:inScheme rdf:resource="http://rs.gbif.org/terms/ncd/ncdType.rdf"/>
    <skos:example>Papers and records, field logs, photographs, etc.</skos:example>
    <skos:note></skos:note>
    <skos:editorialNote></skos:editorialNote>	
  </skos:Concept>

  <skos:Concept rdf:about="http://rs.tdwg.org/ontology/voc/CollectionType#art">
    <skos:prefLabel xml:lang="en">Art</skos:prefLabel>
    <dc:description xml:lang="en">Representational and impressionistic works from the human hand.</dc:description>
    <vann:termGroup rdf:resource="http://rs.tdwg.org/ontology/voc/CollectionType#CollectionTypeTerm"/>
    <rdf:type rdf:resource="rdfs:Datatype"/>
    <rdfs:isDefinedBy rdf:resource="http://rs.tdwg.org/ontology/voc/CollectionType"/>
    <skos:inScheme rdf:resource="http://rs.gbif.org/terms/ncd/ncdType.rdf"/>
    <skos:example>Paintings, drawings, scientific illustrations.</skos:example>
    <skos:note></skos:note>
    <skos:editorialNote></skos:editorialNote>	
  </skos:Concept>

  <skos:Concept rdf:about="http://rs.tdwg.org/ontology/voc/CollectionType#audio">
    <skos:prefLabel xml:lang="en">Audio</skos:prefLabel>
    <dc:description xml:lang="en">Recorded auditory phenomena.</dc:description>
    <vann:termGroup rdf:resource="http://rs.tdwg.org/ontology/voc/CollectionType#CollectionTypeTerm"/>
    <rdf:type rdf:resource="rdfs:Datatype"/>
    <rdfs:isDefinedBy rdf:resource="http://rs.tdwg.org/ontology/voc/CollectionType"/>
    <skos:inScheme rdf:resource="http://rs.gbif.org/terms/ncd/ncdType.rdf"/>
    <skos:example>Amphibian mating calls.</skos:example>
    <skos:note></skos:note>
    <skos:editorialNote></skos:editorialNote>	
  </skos:Concept>

  <skos:Concept rdf:about="http://rs.tdwg.org/ontology/voc/CollectionType#cellCultures">
    <skos:prefLabel xml:lang="en">Cell Cultures</skos:prefLabel>
    <dc:description xml:lang="en">Biological material that is intentionally cultured and maintained in a viable state.</dc:description>
    <vann:termGroup rdf:resource="http://rs.tdwg.org/ontology/voc/CollectionType#CollectionTypeTerm"/>
    <rdf:type rdf:resource="rdfs:Datatype"/>
    <rdfs:isDefinedBy rdf:resource="http://rs.tdwg.org/ontology/voc/CollectionType"/>
    <skos:inScheme rdf:resource="http://rs.gbif.org/terms/ncd/ncdType.rdf"/>
    <skos:example>cell lines, microorganisms.</skos:example>
    <skos:note></skos:note>
    <skos:editorialNote>WARNING: the TDWG Ontology (Collection.rdf) declare all lowercase "cellcultures" as the concept name.</skos:editorialNote>	
  </skos:Concept>

  <skos:Concept rdf:about="http://rs.tdwg.org/ontology/voc/CollectionType#electronic">
    <skos:prefLabel xml:lang="en">Electronic </skos:prefLabel>
    <dc:description xml:lang="en">Data or information that has been captured and fixed for storage and manipulation in an automated system and that requires the use of the system to render it intelligible by a person</dc:description>
    <vann:termGroup rdf:resource="http://rs.tdwg.org/ontology/voc/CollectionType#CollectionTypeTerm"/>
    <rdf:type rdf:resource="rdfs:Datatype"/>
    <rdfs:isDefinedBy rdf:resource="http://rs.tdwg.org/ontology/voc/CollectionType"/>
    <skos:inScheme rdf:resource="http://rs.gbif.org/terms/ncd/ncdType.rdf"/>
    <skos:example>Electronic documents, web sites, databases.</skos:example>
    <skos:note></skos:note>
    <skos:editorialNote></skos:editorialNote>	
  </skos:Concept>

  <skos:Concept rdf:about="http://rs.tdwg.org/ontology/voc/CollectionType#facsimiles">
    <skos:prefLabel xml:lang="en">Facsimiles</skos:prefLabel>
    <dc:description xml:lang="en">Accurate representations of the original item; may be naturally occurring or manufactured.</dc:description>
    <vann:termGroup rdf:resource="http://rs.tdwg.org/ontology/voc/CollectionType#CollectionTypeTerm"/>
    <rdf:type rdf:resource="rdfs:Datatype"/>
    <rdfs:isDefinedBy rdf:resource="http://rs.tdwg.org/ontology/voc/CollectionType"/>
    <skos:inScheme rdf:resource="http://rs.gbif.org/terms/ncd/ncdType.rdf"/>
    <skos:example>Endocasts, moulds.</skos:example>
    <skos:note></skos:note>
    <skos:editorialNote></skos:editorialNote>	
  </skos:Concept>

  <skos:Concept rdf:about="http://rs.tdwg.org/ontology/voc/CollectionType#fossils">
    <skos:prefLabel xml:lang="en">Fossils</skos:prefLabel>
    <dc:description xml:lang="en">Biological specimens that are preserved through geological processes.</dc:description>
    <vann:termGroup rdf:resource="http://rs.tdwg.org/ontology/voc/CollectionType#CollectionTypeTerm"/>
    <rdf:type rdf:resource="rdfs:Datatype"/>
    <rdfs:isDefinedBy rdf:resource="http://rs.tdwg.org/ontology/voc/CollectionType"/>
    <skos:inScheme rdf:resource="http://rs.gbif.org/terms/ncd/ncdType.rdf"/>
    <skos:example>Tyrannosaurus Rex.</skos:example>
    <skos:note></skos:note>
    <skos:editorialNote></skos:editorialNote>	
  </skos:Concept>

  <skos:Concept rdf:about="http://rs.tdwg.org/ontology/voc/CollectionType#genetic">
    <skos:prefLabel xml:lang="en">Genetic</skos:prefLabel>
    <dc:description xml:lang="en">Stored or banked biological samples for future use.</dc:description>
    <vann:termGroup rdf:resource="http://rs.tdwg.org/ontology/voc/CollectionType#CollectionTypeTerm"/>
    <rdf:type rdf:resource="rdfs:Datatype"/>
    <rdfs:isDefinedBy rdf:resource="http://rs.tdwg.org/ontology/voc/CollectionType"/>
    <skos:inScheme rdf:resource="http://rs.gbif.org/terms/ncd/ncdType.rdf"/>
    <skos:example>Germplasm, seed, blood, DNA.</skos:example>
    <skos:note></skos:note>
    <skos:editorialNote></skos:editorialNote>	
  </skos:Concept>

  <skos:Concept rdf:about="http://rs.tdwg.org/ontology/voc/CollectionType#geological">
    <skos:prefLabel xml:lang="en">Geological</skos:prefLabel>
    <dc:description xml:lang="en">Collections of rocks and gemstones</dc:description>
    <vann:termGroup rdf:resource="http://rs.tdwg.org/ontology/voc/CollectionType#CollectionTypeTerm"/>
    <rdf:type rdf:resource="rdfs:Datatype"/>
    <rdfs:isDefinedBy rdf:resource="http://rs.tdwg.org/ontology/voc/CollectionType"/>
    <skos:inScheme rdf:resource="http://rs.gbif.org/terms/ncd/ncdType.rdf"/>
    <skos:example></skos:example>
    <skos:note></skos:note>
    <skos:editorialNote></skos:editorialNote>	
  </skos:Concept>

  <skos:Concept rdf:about="http://rs.tdwg.org/ontology/voc/CollectionType#herbarium">
    <skos:prefLabel xml:lang="en">Herbarium</skos:prefLabel>
    <dc:description xml:lang="en">Collections of preserved plant material</dc:description>
    <vann:termGroup rdf:resource="http://rs.tdwg.org/ontology/voc/CollectionType#CollectionTypeTerm"/>
    <rdf:type rdf:resource="rdfs:Datatype"/>
    <rdfs:isDefinedBy rdf:resource="http://rs.tdwg.org/ontology/voc/CollectionType"/>
    <skos:inScheme rdf:resource="http://rs.gbif.org/terms/ncd/ncdType.rdf"/>
    <skos:example></skos:example>
    <skos:note></skos:note>
    <skos:editorialNote></skos:editorialNote>	
  </skos:Concept>

  <skos:Concept rdf:about="http://rs.tdwg.org/ontology/voc/CollectionType#living">
    <skos:prefLabel xml:lang="en">Living </skos:prefLabel>
    <dc:description xml:lang="en">Collections that grow and reproduce, including botanic gardens, zoos and aquaria.</dc:description>
    <vann:termGroup rdf:resource="http://rs.tdwg.org/ontology/voc/CollectionType#CollectionTypeTerm"/>
    <rdf:type rdf:resource="rdfs:Datatype"/>
    <rdfs:isDefinedBy rdf:resource="http://rs.tdwg.org/ontology/voc/CollectionType"/>
    <skos:inScheme rdf:resource="http://rs.gbif.org/terms/ncd/ncdType.rdf"/>
    <skos:example>Plants, fish.</skos:example>
    <skos:note></skos:note>
    <skos:editorialNote></skos:editorialNote>	
  </skos:Concept>

  <skos:Concept rdf:about="http://rs.tdwg.org/ontology/voc/CollectionType#manuscripts">
    <skos:prefLabel xml:lang="en">Manuscripts</skos:prefLabel>
    <dc:description xml:lang="en">Collections of manuscripts</dc:description>
    <vann:termGroup rdf:resource="http://rs.tdwg.org/ontology/voc/CollectionType#CollectionTypeTerm"/>
    <rdf:type rdf:resource="rdfs:Datatype"/>
    <rdfs:isDefinedBy rdf:resource="http://rs.tdwg.org/ontology/voc/CollectionType"/>
    <skos:inScheme rdf:resource="http://rs.gbif.org/terms/ncd/ncdType.rdf"/>
    <skos:example>Letters, field note books</skos:example>
    <skos:note></skos:note>
    <skos:editorialNote></skos:editorialNote>	
  </skos:Concept>

  <skos:Concept rdf:about="http://rs.tdwg.org/ontology/voc/CollectionType#mineralogical">
    <skos:prefLabel xml:lang="en">Mineralogical</skos:prefLabel>
    <dc:description xml:lang="en">Collections of minerals</dc:description>
    <vann:termGroup rdf:resource="http://rs.tdwg.org/ontology/voc/CollectionType#CollectionTypeTerm"/>
    <rdf:type rdf:resource="rdfs:Datatype"/>
    <rdfs:isDefinedBy rdf:resource="http://rs.tdwg.org/ontology/voc/CollectionType"/>
    <skos:inScheme rdf:resource="http://rs.gbif.org/terms/ncd/ncdType.rdf"/>
    <skos:example>Rocks, minerals, gemstones, meteorites</skos:example>
    <skos:note></skos:note>
    <skos:editorialNote></skos:editorialNote>	
  </skos:Concept>

  <skos:Concept rdf:about="http://rs.tdwg.org/ontology/voc/CollectionType#observations">
    <skos:prefLabel xml:lang="en">Observations </skos:prefLabel>
    <dc:description xml:lang="en">Recorded visual phenomena, may be observation or measurement.</dc:description>
    <vann:termGroup rdf:resource="http://rs.tdwg.org/ontology/voc/CollectionType#CollectionTypeTerm"/>
    <rdf:type rdf:resource="rdfs:Datatype"/>
    <rdfs:isDefinedBy rdf:resource="http://rs.tdwg.org/ontology/voc/CollectionType"/>
    <skos:inScheme rdf:resource="http://rs.gbif.org/terms/ncd/ncdType.rdf"/>
    <skos:example>Avian mating displays.</skos:example>
    <skos:note></skos:note>
    <skos:editorialNote></skos:editorialNote>	
  </skos:Concept>

  <skos:Concept rdf:about="http://rs.tdwg.org/ontology/voc/CollectionType#preserved">
    <skos:prefLabel xml:lang="en">Preserved</skos:prefLabel>
    <dc:description xml:lang="en">Biological specimens that are preserved indefinitely.</dc:description>
    <vann:termGroup rdf:resource="http://rs.tdwg.org/ontology/voc/CollectionType#CollectionTypeTerm"/>
    <rdf:type rdf:resource="rdfs:Datatype"/>
    <rdfs:isDefinedBy rdf:resource="http://rs.tdwg.org/ontology/voc/CollectionType"/>
    <skos:inScheme rdf:resource="http://rs.gbif.org/terms/ncd/ncdType.rdf"/>
    <skos:example>Pinned butterfly, herbarium specimen, skeleton, skin, fluid-preserved invertebrate.</skos:example>
    <skos:note></skos:note>
    <skos:editorialNote></skos:editorialNote>	
  </skos:Concept>

  <skos:Concept rdf:about="http://rs.tdwg.org/ontology/voc/CollectionType#products">
    <skos:prefLabel xml:lang="en">Products</skos:prefLabel>
    <dc:description xml:lang="en">Specimens that result from biological functions or behaviours.</dc:description>
    <vann:termGroup rdf:resource="http://rs.tdwg.org/ontology/voc/CollectionType#CollectionTypeTerm"/>
    <rdf:type rdf:resource="rdfs:Datatype"/>
    <rdfs:isDefinedBy rdf:resource="http://rs.tdwg.org/ontology/voc/CollectionType"/>
    <skos:inScheme rdf:resource="http://rs.gbif.org/terms/ncd/ncdType.rdf"/>
    <skos:example>Nests, webs, moults</skos:example>
    <skos:note></skos:note>
    <skos:editorialNote>WARNING: the NCD 0.9 uses term ID ncdType:audio here.</skos:editorialNote>	
  </skos:Concept>
  <!-- WARNING :: the NCD 0.9 uses term ID ncdType:audio here. -->

  <skos:Concept rdf:about="http://rs.tdwg.org/ontology/voc/CollectionType#specimens">
    <skos:prefLabel xml:lang="en">Specimens</skos:prefLabel>
    <dc:description xml:lang="en">Relating to a particular grouping of biological or geological specimens.</dc:description>
    <vann:termGroup rdf:resource="http://rs.tdwg.org/ontology/voc/CollectionType#CollectionTypeTerm"/>
    <rdf:type rdf:resource="rdfs:Datatype"/>
    <rdfs:isDefinedBy rdf:resource="http://rs.tdwg.org/ontology/voc/CollectionType"/>
    <skos:inScheme rdf:resource="http://rs.gbif.org/terms/ncd/ncdType.rdf"/>
    <skos:example>North American Mammal collection, lichen collection, arachnid collection.</skos:example>
    <skos:note></skos:note>
    <skos:editorialNote>WARNING: this concept is missing in the TDWG Ontology (CollectionType.rdf), however the TDWG ontology declares ".../collectionType#taxonomic" with the same description as this concept. "...collectionType#taxonomic" is missing in the NCD 0.9 documentation (and not included here).</skos:editorialNote>	
  </skos:Concept>
  <!-- WARNING :: CollectionType:specimens is missing in the TDWG Ontology (CollectionType.rdf), 
  however the TDWG ontology declares ".../collectionType#taxonomic" with the same description as declared for this concept. 
  "...collectionType#taxonomic" is missing in the NCD 0.9 documentation (and not included here). -->

  <skos:Concept rdf:about="http://rs.tdwg.org/ontology/voc/CollectionType#texts">
    <skos:prefLabel xml:lang="en">Texts</skos:prefLabel>
    <dc:description xml:lang="en">A collection of books or other written material, or a list of works.</dc:description>
    <vann:termGroup rdf:resource="http://rs.tdwg.org/ontology/voc/CollectionType#CollectionTypeTerm"/>
    <rdf:type rdf:resource="rdfs:Datatype"/>
    <rdfs:isDefinedBy rdf:resource="http://rs.tdwg.org/ontology/voc/CollectionType"/>
    <skos:inScheme rdf:resource="http://rs.gbif.org/terms/ncd/ncdType.rdf"/>
    <skos:example>Books, journals and other printed publications.</skos:example>
    <skos:note></skos:note>
    <skos:editorialNote></skos:editorialNote>	
  </skos:Concept>

  <skos:Concept rdf:about="http://rs.tdwg.org/ontology/voc/CollectionType#tissue">
    <skos:prefLabel xml:lang="en">Tissue</skos:prefLabel>
    <dc:description xml:lang="en">Plant and/or animal tissues.</dc:description>
    <vann:termGroup rdf:resource="http://rs.tdwg.org/ontology/voc/CollectionType#CollectionTypeTerm"/>
    <rdf:type rdf:resource="rdfs:Datatype"/>
    <rdfs:isDefinedBy rdf:resource="http://rs.tdwg.org/ontology/voc/CollectionType"/>
    <skos:inScheme rdf:resource="http://rs.gbif.org/terms/ncd/ncdType.rdf"/>
    <skos:example>Blood, muscle, ovary, leaf.</skos:example>
    <skos:note></skos:note>
    <skos:editorialNote></skos:editorialNote>	
  </skos:Concept>

  <skos:Concept rdf:about="http://rs.tdwg.org/ontology/voc/CollectionType#visual">
    <skos:prefLabel xml:lang="en">Visual</skos:prefLabel>
    <dc:description xml:lang="en">Visual representations of an item or object produced with the aid of a camera or a digital imaging system.</dc:description>
    <vann:termGroup rdf:resource="http://rs.tdwg.org/ontology/voc/CollectionType#CollectionTypeTerm"/>
    <rdf:type rdf:resource="rdfs:Datatype"/>
    <rdfs:isDefinedBy rdf:resource="http://rs.tdwg.org/ontology/voc/CollectionType"/>
    <skos:inScheme rdf:resource="http://rs.gbif.org/terms/ncd/ncdType.rdf"/>
    <skos:example>Photograph, SEM image, video.</skos:example>
    <skos:note></skos:note>
    <skos:editorialNote></skos:editorialNote>	
