  </skos:Concept>

  <skos:Concept rdf:about="&spi;WoodDecayer">
    <skos:prefLabel xml:lang="en">Wood decayer</skos:prefLabel>
    <skos:definition xml:lang="en">Wood decayer fungus with ability to decompose cellulose and/or lignin, or smaller fragments thereof, unknown which category it belongs to.</skos:definition>
    <rdfs:seeAlso rdf:resource="http://vocabularies.gbif.org/vocabularies/Interaction/WoodDecayer"/>
    <dcterms:issued>2012-03-21</dcterms:issued>
    <dcterms:modified>2012-03-22</dcterms:modified>
