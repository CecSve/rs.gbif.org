  <skos:Concept rdf:about="&spi;SugarFungus">
    <skos:prefLabel xml:lang="en">sugar fungus</skos:prefLabel>
    <skos:definition xml:lang="en">Fungus that live on sugars and carbon compounds simpler than cellulose in the initial decomposition stage.</skos:definition>
    <rdfs:seeAlso rdf:resource="http://vocabularies.gbif.org/Interaction/SugarFungus"/>
    <dcterms:issued>2012-03-21</dcterms:issued>
    <dcterms:modified>2012-03-22</dcterms:modified>
