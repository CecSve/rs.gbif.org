    <rdfs:isDefinedBy rdf:resource="&spi;"/>
    <vs:term_status>testing</vs:term_status>
  </skos:Concept>

  <skos:Concept rdf:about="&spi;WoodDecayerResidual">
    <skos:prefLabel xml:lang="en">Wood decayer, residual</skos:prefLabel>
    <skos:definition xml:lang="en">Wood decayer fungus with ability to decompose cellulose and/or lignin, or smaller fragments thereof, residual category.</skos:definition>
    <rdfs:seeAlso rdf:resource="http://vocabularies.gbif.org/vocabularies/Interaction/WoodDecayerResidual"/>
    <dcterms:issued>2012-03-21</dcterms:issued>
    <dcterms:modified>2012-03-22</dcterms:modified>
