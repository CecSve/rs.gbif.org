    <rdfs:isDefinedBy rdf:resource="&spi;"/>
    <vs:term_status>testing</vs:term_status>
  </skos:Concept>

  <skos:Concept rdf:about="&spi;OccursAsMycoparasite">
    <skos:prefLabel xml:lang="en">Occurs as mycoparasite</skos:prefLabel>
    <skos:definition xml:lang="en">Fungus that is a parasite on a live wood decaying fungus.</skos:definition>
    <rdfs:seeAlso rdf:resource="http://vocabularies.gbif.org/vocabularies/Interaction/OccursAsMycoparasite"/>
    <dcterms:issued>2012-03-21</dcterms:issued>
    <dcterms:modified>2012-03-22</dcterms:modified>
    <skos:inScheme rdf:resource="&spi;"/>
    <rdfs:isDefinedBy rdf:resource="&spi;"/>
    <vs:term_status>testing</vs:term_status>
  </skos:Concept>

  <skos:Concept rdf:about="&spi;OccursAsMycodecomposer">
    <skos:prefLabel xml:lang="en">Occurs as mycodecomposer</skos:prefLabel>
    <skos:definition xml:lang="en">Fungus that decomposes a dead wood decaying fungus.</skos:definition>
    <rdfs:seeAlso rdf:resource="http://vocabularies.gbif.org/vocabularies/Interaction/OccursAsMycodecomposer"/>
    <dcterms:issued>2012-03-21</dcterms:issued>
    <dcterms:modified>2012-03-22</dcterms:modified>
    <skos:inScheme rdf:resource="&spi;"/>
    <rdfs:isDefinedBy rdf:resource="&spi;"/>
    <vs:term_status>testing</vs:term_status>
  </skos:Concept>

  <skos:Concept rdf:about="&spi;WoodBoringDetrivore">
    <skos:prefLabel xml:lang="en">Wood boring detrivore</skos:prefLabel>
    <skos:definition xml:lang="en">Make distinct galleries in living or dead bark or wood, with or without fungus mycelium.</skos:definition>
    <rdfs:seeAlso rdf:resource="http://vocabularies.gbif.org/vocabularies/Interaction/WoodBoringDetrivore"/>
    <dcterms:issued>2012-03-21</dcterms:issued>
    <dcterms:modified>2012-03-22</dcterms:modified>
    <skos:inScheme rdf:resource="&spi;"/>
    <rdfs:isDefinedBy rdf:resource="&spi;"/>
    <vs:term_status>testing</vs:term_status>
  </skos:Concept>

  <skos:Concept rdf:about="&spi;Detrivore">
    <skos:prefLabel xml:lang="en">Detrivore</skos:prefLabel>
    <skos:definition xml:lang="en">Feeds on woody material (or sap) with or without fungus mycelium. Does not make distinct galleries.</skos:definition>
    <rdfs:seeAlso rdf:resource="http://vocabularies.gbif.org/vocabularies/Interaction/Detrivore"/>
    <dcterms:issued>2012-03-21</dcterms:issued>
    <dcterms:modified>2012-03-22</dcterms:modified>
    <skos:inScheme rdf:resource="&spi;"/>
    <rdfs:isDefinedBy rdf:resource="&spi;"/>
    <vs:term_status>testing</vs:term_status>
  </skos:Concept>

  <skos:Concept rdf:about="&spi;OccursAsMicrobivore">
    <skos:prefLabel xml:lang="en">Occurs as microbivore</skos:prefLabel>
    <skos:definition xml:lang="en">Filter feeders that feed on bacteria and other food particles dissolved in wet media (typically sap).</skos:definition>
    <rdfs:seeAlso rdf:resource="http://vocabularies.gbif.org/vocabularies/Interaction/OccursAsMicrobivore"/>
    <dcterms:issued>2012-03-21</dcterms:issued>
    <dcterms:modified>2012-03-22</dcterms:modified>
    <skos:inScheme rdf:resource="&spi;"/>
    <rdfs:isDefinedBy rdf:resource="&spi;"/>
    <vs:term_status>testing</vs:term_status>
  </skos:Concept>

  <skos:Concept rdf:about="&spi;OccursAsAmbrosiaConsumer">
    <skos:prefLabel xml:lang="en">Occurs as ambrosia consumer</skos:prefLabel>
    <skos:definition xml:lang="en">Wood boring invertebrate feeding on ambrosia fungus.</skos:definition>
    <rdfs:seeAlso rdf:resource="http://vocabularies.gbif.org/vocabularies/Interaction/OccursAsAmbrosiaConsumer"/>
    <dcterms:issued>2012-03-21</dcterms:issued>
    <dcterms:modified>2012-03-22</dcterms:modified>
    <skos:inScheme rdf:resource="&spi;"/>
    <rdfs:isDefinedBy rdf:resource="&spi;"/>
    <vs:term_status>testing</vs:term_status>
  </skos:Concept>

  <skos:Concept rdf:about="&spi;OccursAsFungivore">
    <skos:prefLabel xml:lang="en">Occurs as fungivore</skos:prefLabel>
    <skos:definition xml:lang="en">Invertebrate feeding on live or dead wood-decaying fungi. Either some or all parts of the fungus (mycelium, fruit body or spores).</skos:definition>
    <rdfs:seeAlso rdf:resource="http://vocabularies.gbif.org/vocabularies/Interaction/OccursAsFungivore"/>
    <dcterms:issued>2012-03-21</dcterms:issued>
    <dcterms:modified>2012-03-22</dcterms:modified>
    <skos:inScheme rdf:resource="&spi;"/>
    <rdfs:isDefinedBy rdf:resource="&spi;"/>
    <vs:term_status>testing</vs:term_status>
  </skos:Concept>

  <skos:Concept rdf:about="&spi;OccursAsScavenger">
    <skos:prefLabel xml:lang="en">Occurs as scavenger</skos:prefLabel>
    <skos:definition xml:lang="en">Diet is mix of woody debris, mycelium and dead animals, some also feed on bacteria etc. in decaying matter. Includes insects in hollow trees and bird nests.</skos:definition>
    <rdfs:seeAlso rdf:resource="http://vocabularies.gbif.org/vocabularies/Interaction/OccursAsScavenger"/>
    <dcterms:issued>2012-03-21</dcterms:issued>
    <dcterms:modified>2012-03-22</dcterms:modified>
    <skos:inScheme rdf:resource="&spi;"/>
    <rdfs:isDefinedBy rdf:resource="&spi;"/>
    <vs:term_status>testing</vs:term_status>
  </skos:Concept>

  <skos:Concept rdf:about="&spi;OccursAsOmnivor">
    <skos:prefLabel xml:lang="en">Occurs as omnivor</skos:prefLabel>
    <skos:definition xml:lang="en">A detrivore or fungivore that also predates (occasionally) on live animals, typically larvae/pupae.</skos:definition>
    <rdfs:seeAlso rdf:resource="http://vocabularies.gbif.org/vocabularies/Interaction/OccursAsOmnivor"/>
    <dcterms:issued>2012-03-21</dcterms:issued>
    <dcterms:modified>2012-03-22</dcterms:modified>
    <skos:inScheme rdf:resource="&spi;"/>
    <rdfs:isDefinedBy rdf:resource="&spi;"/>
    <vs:term_status>testing</vs:term_status>
  </skos:Concept>

  <skos:Concept rdf:about="&spi;OccursAsPredator">
    <skos:prefLabel xml:lang="en">Occurs as predator</skos:prefLabel>
    <skos:definition xml:lang="en">Predator, typically on insect larvae but also on nematodes (predatory insects and woodpeckers, ant-eaters, etc.; even predatory fungi).</skos:definition>
    <rdfs:seeAlso rdf:resource="http://vocabularies.gbif.org/vocabularies/Interaction/OccursAsPredator"/>
    <dcterms:issued>2012-03-21</dcterms:issued>
    <dcterms:modified>2012-03-22</dcterms:modified>
    <skos:inScheme rdf:resource="&spi;"/>
    <rdfs:isDefinedBy rdf:resource="&spi;"/>
    <vs:term_status>testing</vs:term_status>
  </skos:Concept>

  <skos:Concept rdf:about="&spi;OccursAsParasite">
    <skos:prefLabel xml:lang="en">Occurs as parasite</skos:prefLabel>
    <skos:definition xml:lang="en">Parasite on other saproxylic invertebrates.</skos:definition>
    <rdfs:seeAlso rdf:resource="http://vocabularies.gbif.org/vocabularies/Interaction/OccursAsParasite"/>
    <dcterms:issued>2012-03-21</dcterms:issued>
    <dcterms:modified>2012-03-22</dcterms:modified>
    <skos:inScheme rdf:resource="&spi;"/>
    <rdfs:isDefinedBy rdf:resource="&spi;"/>
    <vs:term_status>testing</vs:term_status>
  </skos:Concept>

  <skos:Concept rdf:about="&spi;OccursAsParasitoid">
    <skos:prefLabel xml:lang="en">Occurs as parasitoid</skos:prefLabel>
    <skos:definition xml:lang="en">Parasitoid on other saproxylic invertebrates.</skos:definition>
    <rdfs:seeAlso rdf:resource="http://vocabularies.gbif.org/vocabularies/Interaction/OccursAsParasitoid"/>
    <dcterms:issued>2012-03-21</dcterms:issued>
    <dcterms:modified>2012-03-22</dcterms:modified>
    <skos:inScheme rdf:resource="&spi;"/>
    <rdfs:isDefinedBy rdf:resource="&spi;"/>
    <vs:term_status>testing</vs:term_status>
  </skos:Concept>

  <skos:Concept rdf:about="&spi;OccursAsHyperparasitoid">
    <skos:prefLabel xml:lang="en">Occurs as hyperparasitoid</skos:prefLabel>
    <skos:definition xml:lang="en">Parasitoid on other parasitoids.</skos:definition>
    <rdfs:seeAlso rdf:resource="http://vocabularies.gbif.org/vocabularies/Interaction/OccursAsHyperparasitoid"/>
    <dcterms:issued>2012-03-21</dcterms:issued>
    <dcterms:modified>2012-03-22</dcterms:modified>
    <skos:inScheme rdf:resource="&spi;"/>
    <rdfs:isDefinedBy rdf:resource="&spi;"/>
    <vs:term_status>testing</vs:term_status>
  </skos:Concept>

  <skos:Concept rdf:about="&spi;OccursAsCleptoparasite">
    <skos:prefLabel xml:lang="en">Occurs as cleptoparasite</skos:prefLabel>
    <skos:definition xml:lang="en">Cleptoparasite on endoxylic species.</skos:definition>
    <rdfs:seeAlso rdf:resource="http://vocabularies.gbif.org/vocabularies/Interaction/OccursAsCleptoparasite"/>
    <dcterms:issued>2012-03-21</dcterms:issued>
    <dcterms:modified>2012-03-22</dcterms:modified>
    <skos:inScheme rdf:resource="&spi;"/>
    <rdfs:isDefinedBy rdf:resource="&spi;"/>
    <vs:term_status>testing</vs:term_status>
  </skos:Concept>

  <skos:Concept rdf:about="&spi;AssociatedSpecies">
    <skos:prefLabel xml:lang="en">Associated species</skos:prefLabel>
    <skos:definition xml:lang="en">Lives in dead wood, but functional role is unknown or unclear.</skos:definition>
    <rdfs:seeAlso rdf:resource="http://vocabularies.gbif.org/vocabularies/Interaction/AssociatedSpecies"/>
    <dcterms:issued>2012-03-21</dcterms:issued>
    <dcterms:modified>2012-03-22</dcterms:modified>
    <skos:inScheme rdf:resource="&spi;"/>
    <rdfs:isDefinedBy rdf:resource="&spi;"/>
    <vs:term_status>testing</vs:term_status>
  </skos:Concept>

  <skos:Concept rdf:about="&spi;Epixylic">
    <skos:prefLabel xml:lang="en">Epixylic</skos:prefLabel>
    <skos:definition xml:lang="en">Grows on surface of dead wood or bark, but is not a wood decomposer (lichens, mosses, mycorrhiza fungi).</skos:definition>
    <rdfs:seeAlso rdf:resource="http://vocabularies.gbif.org/vocabularies/Interaction/Epixylic"/>
    <dcterms:issued>2012-03-21</dcterms:issued>
    <dcterms:modified>2012-03-22</dcterms:modified>
    <skos:inScheme rdf:resource="&spi;"/>
    <rdfs:isDefinedBy rdf:resource="&spi;"/>
    <vs:term_status>testing</vs:term_status>
  </skos:Concept>

  <skos:Concept rdf:about="&spi;WoodNester">
    <skos:prefLabel xml:lang="en">Wood nester</skos:prefLabel>
    <skos:definition xml:lang="en">Nest sites inside dead wood, feeding disconnected from wood (many birds and insects).</skos:definition>
    <rdfs:seeAlso rdf:resource="http://vocabularies.gbif.org/vocabularies/Interaction/WoodNester"/>
    <dcterms:issued>2012-03-21</dcterms:issued>
    <dcterms:modified>2012-03-22</dcterms:modified>
    <skos:inScheme rdf:resource="&spi;"/>
    <rdfs:isDefinedBy rdf:resource="&spi;"/>
    <vs:term_status>testing</vs:term_status>
  </skos:Concept>


<!-- ### Agents, Organization and Person ###
 Providing information about the organization and the persons who contributed
 to creating or maintaining the Vocabulary of Terms is a good practice.
 When using dcterms:creator, :contributor, :publisher etc these have rdfs:range 
 dcterms:Agent and will therefore specify the values to be non-literal resources.

 foaf:organization and foaf:Person entities are described by:
  foaf:name					(full name, institute or person)
  foaf:nickname				(reporting institute acronyms here)
  dcterms:identifier		(eg. from the Biodiversity Collections Index [BCI])
  @rdf:datatype				(datatype property, BCI-ID)
  foaf:homepage				(institute home page, www)
  foaf:member				(list persons affiliated)
  foaf:mbox					(mailbox)
  foaf:mbox_sha1sum			(md5 hash sum based on the mailbox [md5 -s email])
 -->

