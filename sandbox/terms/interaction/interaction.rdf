    <rdfs:isDefinedBy rdf:resource="&spi;"/>
    <vs:term_status>testing</vs:term_status>
  </skos:Concept>

  <skos:Concept rdf:about="&spi;WoodDecayerBrownRot">
    <skos:prefLabel xml:lang="en">Wood decayer, brown rot</skos:prefLabel>
    <skos:definition xml:lang="en">Wood decayer fungus with ability to decompose cellulose and/or lignin, or smaller fragments thereof, brown rot category.</skos:definition>
    <rdfs:seeAlso rdf:resource="http://vocabularies.gbif.org/vocabularies/Interaction/WoodDecayerBrownRot"/>
    <dcterms:issued>2012-03-21</dcterms:issued>
    <dcterms:modified>2012-03-22</dcterms:modified>
    <skos:inScheme rdf:resource="&spi;"/>
    <rdfs:isDefinedBy rdf:resource="&spi;"/>
    <vs:term_status>testing</vs:term_status>
  </skos:Concept>

  <skos:Concept rdf:about="&spi;WoodDecayerWhiteRot">
    <skos:prefLabel xml:lang="en">Wood decayer, white rot</skos:prefLabel>
    <skos:definition xml:lang="en">Wood decayer fungus with ability to decompose cellulose and/or lignin, or smaller fragments thereof, white rot category.</skos:definition>
    <rdfs:seeAlso rdf:resource="http://vocabularies.gbif.org/vocabularies/Interaction/WoodDecayerWhiteRot"/>
    <dcterms:issued>2012-03-21</dcterms:issued>
    <dcterms:modified>2012-03-22</dcterms:modified>
    <skos:inScheme rdf:resource="&spi;"/>
    <rdfs:isDefinedBy rdf:resource="&spi;"/>
    <vs:term_status>testing</vs:term_status>
  </skos:Concept>

  <skos:Concept rdf:about="&spi;WoodDecayerSoftRot">
    <skos:prefLabel xml:lang="en">Wood decayer, soft rot</skos:prefLabel>
    <skos:definition xml:lang="en">Wood decayer fungus with ability to decompose cellulose and/or lignin, or smaller fragments thereof, soft rot category.</skos:definition>
    <rdfs:seeAlso rdf:resource="http://vocabularies.gbif.org/vocabularies/Interaction/WoodDecayerSoftRot"/>
    <dcterms:issued>2012-03-21</dcterms:issued>
    <dcterms:modified>2012-03-22</dcterms:modified>
